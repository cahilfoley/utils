{
  "name": "@cahil/utils",
  "version": "1.0.0-alpha.2",
  "description": "Various utility functions that might come in handy when building web apps",
  "main": "index.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "utils"
  ],
  "author": "Cahil Foley",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cahilfoley/utils.git"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "type-check": "tsc --noEmit",
    "build:amd": "tsc -p amd.tsconfig.json",
    "build:cjs": "tsc -p cjs.tsconfig.json --emitDeclarationOnly && babel src -d dist -x \".ts,.tsx\" -p cjs.tsconfig.json",
    "build:es": "tsc -p es.tsconfig.json",
    "build:umd": "tsc -p umd.tsconfig.json",
    "build:docs": "typedoc --options typedoc.json src/",
    "build": "run-p build:cjs build:es build:amd build:umd && ts-node scripts/copyPackage",
    "clean": "rimraf dist coverage docs",
    "commit": "git-cz",
    "deploy:docs": "ts-node scripts/ghPagesPublish",
    "prepublishOnly": "yarn clean && yarn test && yarn build",
    "postpublish": "yarn clean",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node scripts/semanticReleasePrepare",
    "test": "yarn type-check && jest",
    "prepush": "npm run test:prod && npm run build",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "travis-deploy-once": "travis-deploy-once",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.2",
    "babel-jest": "^23.4.2",
    "colors": "^1.3.2",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.9",
    "shelljs": "^0.8.2",
    "travis-deploy-once": "^5.0.3",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^7.1.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "lodash.partial": "^4.2.1"
  },
  "peerDependencies": {
    "jest": "^22 || ^23"
  }
}
