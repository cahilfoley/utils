{
  "name": "@cahil/utils",
  "version": "0.1.1",
  "description": "Various utility functions that might come in handy when building web apps",
  "main": "dist/utils.js",
  "module": "./src/",
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc --exclude **/node_modules/**/*.ts --module umd --ignoreCompilerErrors --target ES6 --out doc/ src/",
    "bundle": "rollup -c",
    "clean": "yarn rimraf dist coverage",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "postpublish": "npm run clean",
    "test": "jest"
  },
  "keywords": [
    "utils"
  ],
  "author": "Cahil Foley",
  "license": "MIT",
  "peerDependencies": {
    "lodash": "^4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "fs-extra": "^7.0.0",
    "jest": "^23.5.0",
    "prettier-tslint": "^0.4.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typedoc": "^0.11.1",
    "typescript": "^3.0.1"
  }
}