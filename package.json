{
  "name": "@cahil/utils",
  "version": "1.0.3-1",
  "description": "Various utility functions that might come in handy when building things in JavaScript",
  "main": "index.amd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "utils"
  ],
  "author": "Cahil Foley",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cahilfoley/utils.git"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "type-check": "tsc --noEmit",
    "build:amd": "tsc -p amd.tsconfig.json",
    "build:cjs": "tsc -p cjs.tsconfig.json --emitDeclarationOnly && babel src -d dist -x \".ts,.tsx\" -p cjs.tsconfig.json",
    "build:es": "tsc -p es.tsconfig.json",
    "build:docs": "typedoc --options typedoc.json src/",
    "build:docsjson": "typedoc --options typedoc-json.json src/",
    "build:readme": "typedoc --options typedoc-md.json src/",
    "build": "yarn clean && yarn build:readme && run-p build:cjs build:es build:amd && ts-node scripts/copyPackage",
    "clean": "rimraf dist coverage docs",
    "deploy:docs": "ts-node scripts/ghPagesPublish",
    "prepublishOnly": "yarn clean && yarn test && yarn build",
    "postpublish": "yarn clean",
    "test": "yarn type-check && jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "babel-jest": "^23.6.0",
    "colors": "^1.3.2",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.12.0",
    "typedoc-plugin-markdown": "^1.1.15",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "lodash.partial": "^4.2.1"
  },
  "peerDependencies": {
    "jest": "^22 || ^23"
  }
}
