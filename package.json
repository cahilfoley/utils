{
  "name": "@cahil/utils",
  "version": "0.1.8",
  "description": "Various utility functions that might come in handy when building web apps",
  "main": "index.js",
  "module": "es/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cahilfoley/utils.git"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:amd": "tsc -p amd.tsconfig.json",
    "build:cjs": "tsc -p cjs.tsconfig.json --emitDeclarationOnly && babel src -d dist -x \".ts,.tsx\" -p cjs.tsconfig.json",
    "build:es": "tsc -p es.tsconfig.json",
    "build:docs": "typedoc --options typedoc.json src/",
    "build": "run-p build:cjs build:es build:amd && node scripts/copyPackage",
    "clean": "rimraf dist coverage docs",
    "prepublishOnly": "yarn clean && yarn test && yarn build",
    "postpublish": "yarn clean",
    "test": "yarn type-check && jest",
    "lint": "tslint 'src/**/*.{ts,tsx}'"
  },
  "keywords": [
    "utils"
  ],
  "author": "Cahil Foley",
  "license": "MIT",
  "dependencies": {
    "jest": "^23.5.0",
    "lodash": "^4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "coveralls": "^3.0.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1"
  }
}